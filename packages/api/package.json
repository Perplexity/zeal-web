{
  "name": "api",
  "version": "0.0.0",
  "scripts": {
    "build": "ts-node build.ts",
    "lint": "eslint ./src --ext .ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json"
  },
  "nodemonConfig": {
    "exec": "ts-node -r tsconfig-paths/register ./src",
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "watch": [
      "src"
    ]
  },
  "eslintIgnore": [
    "src/public/",
    "build.ts"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bull": "^3.22.9",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "find": "^0.3.0",
    "fs-extra": "^9.1.0",
    "helmet": "^4.5.0",
    "http-status-codes": "^2.1.4",
    "jasmine": "^3.7.0",
    "jet-logger": "^1.0.4",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "tsconfig-paths": "^3.9.0"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.1",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.11",
    "@types/jasmine": "^3.6.10",
    "@types/jsonfile": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
